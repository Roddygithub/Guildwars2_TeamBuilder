# Configuration de la base de données
DATABASE_URL=postgresql://user:password@localhost:5432/gw2_teambuilder

# Configuration de l'application
ENVIRONMENT=development
LOG_LEVEL=INFO

# Clé API Guild Wars 2 (optionnel)
GW2_API_KEY=your_api_key_here

# Configuration du déploiement Netlify
# Ces variables doivent être configurées dans les paramètres du site Netlify
NETLIFY=true

# Configuration CORS (si nécessaire)
FRONTEND_URL=http://localhost:3000
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
