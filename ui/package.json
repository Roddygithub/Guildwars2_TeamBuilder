    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-checker": "^0.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
