    "axios": "^1.6.0",
    "framer-motion": "^10.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
